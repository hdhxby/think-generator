		<url>http://www.hdhxby.com</url>
	</organization>
	<licenses>
		<license>
			<name>Apache License, Version 2.0</name>
			<url>http://www.apache.org/licenses/LICENSE-2.0</url>
		</license>
	</licenses>
	<scm>
		<url>https://github.com/hdhxby/think</url>
	</scm>
	<developers>
		<developer>
			<id>lixiaobin</id>
			<name>lixiaobin</name>
			<email>hdhxby@qq.com</email>
			<organization>Pivotal Software, Inc.</organization>
			<organizationUrl>http://www.spring.io</organizationUrl>
			<roles>
				<role>Project lead</role>
			</roles>
		</developer>
	</developers>
	<modules>
		<module>think-jdbc</module>
		<module>think-generator</module>
